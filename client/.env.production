# 生产环境配置
# API Base URL - 后端服务地址（需要替换为实际的生产环境地址）
EXPO_PUBLIC_API_URL=https://api.contract-assistant.com/api

# 调试模式
EXPO_PUBLIC_DEBUG=false

# 应用环境
EXPO_PUBLIC_ENV=production

# 上传文件大小限制 (MB)
EXPO_PUBLIC_MAX_FILE_SIZE=10

# API 超时时间 (ms)
EXPO_PUBLIC_API_TIMEOUT=30000

# 是否启用日志
EXPO_PUBLIC_ENABLE_LOGGING=false

# 分析轮询间隔 (ms)
EXPO_PUBLIC_POLLING_INTERVAL=5000
